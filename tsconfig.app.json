/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@display/*": ["src/app/display/*"],
      "@resume/*": ["src/app/resume/*"],
      "@layout/*": ["src/app/layout/*"],
      "@form/*": ["src/app/form/*"]
    },
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
}
